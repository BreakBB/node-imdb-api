{
  "author": "William Orr <will@worrbase.com> (http://worrbase.com/)",
  "name": "imdb-api",
  "description": "Queries unofficial imdb APIs to get movie and television information from imdb",
  "version": "3.1.0",
  "main": "lib/imdb.js",
  "homepage": "https://github.com/worr/node-imdb-api",
  "repository": {
    "type": "git",
    "url": "https://github.com/worr/node-imdb-api"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "prepublish": "npm run build",
    "lint": "tslint --project tsconfig.json",
    "test": "nodeunit test",
    "check": "npm run lint && npm run test",
    "docs": "typedoc --out ./doc --ignoreCompilerErrors src/imdb.ts",
    "clean": "rm -rf node_modules lib"
  },
  "devDependencies": {
    "nock": "8.x.x",
    "nodeunit": "0.x.x",
    "tslint": "^3.15.1",
    "typedoc": "^0.7.1",
    "typescript": "^2.0.0"
  },
  "optionalDependencies": {},
  "engines": {
    "node": ">=4.0"
  },
  "keywords": [
    "imdb",
    "movies",
    "search",
    "tv",
    "television"
  ],
  "dependencies": {
    "es6-promise": "^3.2.1",
    "request": "^2.74.0",
    "request-promise": "^4.1.1"
  }
}
